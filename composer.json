{
    "name": "wearemarketing/skeleton-kata-php",
    "description": "Skeleton Kata PHP",
    "keywords": ["kata","TDD"],
    "type": "library",
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0",
        "phpspec/phpspec": "^5.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "WAM Develop Team",
            "email": "develop@wearemarketing.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "spec\\": "spec/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
